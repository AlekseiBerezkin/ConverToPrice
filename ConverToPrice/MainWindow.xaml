<Window x:Class="ConverToPrice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConverToPrice"
        mc:Ignorable="d"
        Title="BaseToPrice" Height="450" Width="411">
    <Grid>
        <Button x:Name="ButtonOpenXlsx" Content="Сканировать исходный файл" HorizontalAlignment="Center" Margin="0,24,0,0" VerticalAlignment="Top" Width="191" Click="ButtonOpenXlsx_Click"/>
        <Label x:Name="LabelCurrentFileName" Content="Журнал" HorizontalAlignment="Left" Margin="10,73,0,0" VerticalAlignment="Top"/>
        <Button x:Name="ButtonRun" Content="Преобразовать исходные данные" HorizontalAlignment="Center" Margin="0,58,0,0" VerticalAlignment="Top" Click="ButtonRun_Click" IsEnabled="False"/>
        <ListBox x:Name="Jurnal" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,104,10,10"/>

    </Grid>
</Window>
